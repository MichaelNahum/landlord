<h2>Enter new digs here, then hit it.</h2>
<form action="/new" method="post">
  <!-- "add_apt is being read as the url. " -->
  <label for="apartment[address]">Address:</label>
  <input name="apartment[address]" value="Address">

  <label for="apartment[monthly_rent]">Rent:</label>
  <input name="apartment[monthly_rent]" value="Rent">

  <label for="apartment[sqft]">Square Ft:</label>
  <input name="apartment[sqft]" value="Square Feet">

  <label for="apartment[num_beds]">Number of Beds:</label>
  <input name="apartment[num_beds]" value="Number of Beds">

  <label for="apartment[num_baths]">Number of Baths:</label>
  <input name="apartment[num_baths]" value="Number of Bathrooms">

  <input type="submit" value="Enter">
</form>
